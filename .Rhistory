unique(dats$ProjectLabel)
dats$Source = ifelse(dats$ProjectLabel %in% c("NRSA0809",
"NRSA1314",
"WSA"),
"NRSA",
"NAWQA")
ggplot(dats, aes(x = Longitude_dd, y = Latitude_dd))+
facet_wrap(~CollectionYear)+
borders()+
borders("state")+
geom_point(aes(color = Source))+
coord_cartesian(ylim = c(25, 50),
xlim = c(-130, -60))
ggplot(dats, aes(x = Longitude_dd, y = Latitude_dd))+
facet_wrap(~CollectionYear)+
borders()+
borders("state")+
geom_point(aes(color = Source),
alpha = 0.3)+
coord_cartesian(ylim = c(25, 50),
xlim = c(-130, -60))
