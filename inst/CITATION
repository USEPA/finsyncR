citHeader("To cite StreamData in publications use:")

citEntry(
  entry = "Unpublished",
  title = "{StreamData}: R Package to Access Harmonized USGS and EPA Aquatic Biomonitoring Datasets",
  author = personList(as.person("Michael Mahon"),
                                   as.person("Devin Jones"),
                                   as.person("Ethan Brown"),
                                   as.person("Terry Brown"),
                                   as.person("Ryan Hill"),
                                   as.person("Stefan Kunz"),
                                   as.person("Samantha Rumschlag")),
  year = "2023",
  note        = "R package version 0.0.0.9421",
 textVersion  =
           paste("Michael Mahon, Devin Jones, Ethan Brown, Terry Brown, Ryan Hill,",
                 "Stefan Kunz, Samantha Rumschlag (2023). {StreamData}: R Package",
                 "to Access Harmonized USGS and EPA Aquatic Biomonitoring Datasets.",
                 "R package version 0.0.0.9421")
)
